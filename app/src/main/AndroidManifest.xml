<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.project.meetple">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".MeetReqListActivity">
        </activity>
        <activity android:name=".myRoomActivity">
        </activity>
        <activity android:name=".MakeMeetRoomActivity">
        </activity>
        <activity android:name=".SelectFriendsActivity">
        </activity>
        <activity android:name=".FriendActivity">
        </activity>
        <activity android:name=".ProfileActivity">
        </activity>
        <activity android:name=".MeetRoomDetail">
        </activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name=".TestActivity"
                android:label="MeetPle">
        </activity>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <activity
                android:name=".SignUpFirstActivity"
                android:label="MeetPle">
        </activity>
        <activity
                android:name=".SignUpSecondActivity"
                android:label="MeetPle">
        </activity>
        <activity
                android:name=".SignUpThirdActivity"
                android:label="MeetPle">
        </activity>
        <activity
                android:name=".SignUpFourthActivity"
                android:label="MeetPle">
        </activity>
        <activity
                android:name=".LoginActivity"
                android:label="MeetPle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".CertifyActivity"
                android:label="MeetPle">
        </activity>
    </application>

</manifest>